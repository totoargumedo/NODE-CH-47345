<div class="d-flex flex-column justify-content-center align-items-center gap-5"
  style="height:100vh;"
>
  <div id="slideIndex" class="carousel slide" data-bs-ride="carousel">
    <div class="carousel-inner ">
      {{#each products}}
        <div class="carousel-item {{#if activeImage}}active{{/if}} " style="height: 350px;">
          <img
            src="{{this.thumbnails}}"
            class="d-block object-fit-cover"
              style="width: 600px;height:450px"
            alt="{{this.title}}"
          />
          <div class="carousel-caption d-none d-md-block">
            <h5>{{this.title}}</h5>
            <p>{{this.description}}</p>
          </div>
          <a href="/products/{{this.id}}" class="stretched-link"></a>
        </div>
      {{/each}}
    </div>
    <button
      class="carousel-control-prev"
      type="button"
      data-bs-target="#slideIndex"
      data-bs-slide="prev"
    >
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Anterior</span>
    </button>
    <button
      class="carousel-control-next"
      type="button"
      data-bs-target="#slideIndex"
      data-bs-slide="next"
    >
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Siguiente</span>
    </button>
  </div>
  <h1>WELCOME STRANGER</h1>
  <h2>este es un div centrado</h2>
  <h3>&#128517;</h3>
</div>
